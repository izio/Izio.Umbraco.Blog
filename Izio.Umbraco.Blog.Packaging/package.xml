<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<umbPackage>
  <files>
    <% files.forEach(function(file) { %>
    <file>
      <guid><%= file.guid %></guid>
      <orgPath><%= file.dir %></orgPath>
      <orgName><%= file.name %></orgName>
    </file>
    <% }); %>
  </files>
  <info>
    <package>
      <name><%= name %></name>
      <version><%= version %></version>
      <iconUrl />
      <license url="<%= licenseUrl%>"><%= license%></license>
      <url><%= url %></url>
      <requirements>
        <major>7</major>
        <minor>5</minor>
        <patch>3</patch>
      </requirements>
    </package>
    <author>
      <name><%= author %></name>
      <website><%= authorUrl %></website>
    </author>
    <readme><![CDATA[<%= readme %>]]></readme>
  </info>
  <DocumentTypes />
  <Templates />
  <Stylesheets />
  <Macros />
  <DictionaryItems />
  <Languages />
  <DataTypes />
  <Actions>
    <Action runat="install" undo="true" alias="Blog_Setup" />
  </Actions>
</umbPackage>